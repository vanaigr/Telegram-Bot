{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true
  },
  "incldue": ["src"],
  "exclude": ["dist", "build", "node_modules", "verifier"]
}
